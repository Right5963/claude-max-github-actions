{
  "mcpServers": {
    "obsidian": {
      "command": "npx",
      "args": [
        "-y",
        "obsidian-mcp-server",
        "--vault-path",
        "G:\\マイドライブ\\Obsidian Vault"
      ]
    },
    "playwright": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "playwright-mcp"
      ]
    },
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "desktop-commander-mcp"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "G:\\マイドライブ\\Obsidian Vault"
      ]
    },
    "browser": {
      "command": "npx",
      "args": [
        "-y",
        "browser-mcp-server"
      ]
    },
    "note-api": {
      "command": "npx",
      "args": [
        "-y",
        "note-api-mcp"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    }
  }
}
